.mui-container-fluid
    .mui-row
        .mui-col-md-8.mui-col-md-offset-2
            #similar-artists.mui-row.artists-list
                .mui-col-lg-12
                    %h3.payment 
                        You are about to order:
                    %h3 
                        - @order.items.each do | item |
                            %span= pluralize(item.quantity, 'ticket')
                            %span to
                            %span= item.item.ticket.campaign.title
                            %br/
                            %br/
                            %br/

                    .cell.example.example2
                        = form_with url: payments_path, local: true, id: 'payment-form' do |form|
                            %div{"data-locale-reversible" => ""}
                            %row
                                .field
                                    %input#example2-name.input.empty{autocomplete: "address-name", "data-tid" => "elements_examples.form.name_placeholder", placeholder: "John Doe", required: "", type: "text"}/
                                    %label{"data-tid" => "elements_examples.form.address_label", for: "example2-address"} Address
                                    .baseline
                            .row
                                .field
                                    %input#example2-address.input.empty{autocomplete: "address-line1", "data-tid" => "elements_examples.form.address_placeholder", placeholder: "185 Berry St", required: "", type: "text"}/
                                    %label{"data-tid" => "elements_examples.form.address_label", for: "example2-address"} Address
                                    .baseline
                            .row{"data-locale-reversible" => ""}
                                .field.half-width
                                    %input#example2-city.input.empty{autocomplete: "address-level2", "data-tid" => "elements_examples.form.city_placeholder", placeholder: "San Francisco", required: "", type: "text"}/
                                    %label{"data-tid" => "elements_examples.form.city_label", for: "example2-city"} City
                                    .baseline
                                .field.half-width
                                    %input#example2-zip.input.empty{autocomplete: "postal-code", "data-tid" => "elements_examples.form.postal_code_placeholder", placeholder: "94107", required: "", type: "text"}/
                                    %label{"data-tid" => "elements_examples.form.postal_code_label", for: "example2-zip"} ZIP
                                    .baseline
                            .row
                                .field
                                    #example2-card-number.input.empty
                                    %label{"data-tid" => "elements_examples.form.card_number_label", for: "example2-card-number"} Card number
                                    .baseline
                            .row
                                .field.half-width
                                    #example2-card-expiry.input.empty
                                    %label{"data-tid" => "elements_examples.form.card_expiry_label", for: "example2-card-expiry"} Expiration
                                    .baseline
                                .field.half-width
                                    #example2-card-cvc.input.empty
                                    %label{"data-tid" => "elements_examples.form.card_cvc_label", for: "example2-card-cvc"} CVC
                                    .baseline
                            %button{class: 'form-submit'} Submit Payment
                            = button_tag 'Cancel', class: 'form-submit', onclick: 'cancelModal(event);' 
                            .error{role: "alert"}
                            %svg{height: "17", viewbox: "0 0 17 17", width: "17", xmlns: "http://www.w3.org/2000/svg"}
                                %path.base{d: "M8.5,17 C3.80557963,17 0,13.1944204 0,8.5 C0,3.80557963 3.80557963,0 8.5,0 C13.1944204,0 17,3.80557963 17,8.5 C17,13.1944204 13.1944204,17 8.5,17 Z", fill: "#000"}
                                %path.glyph{d: "M8.5,7.29791847 L6.12604076,4.92395924 C5.79409512,4.59201359 5.25590488,4.59201359 4.92395924,4.92395924 C4.59201359,5.25590488 4.59201359,5.79409512 4.92395924,6.12604076 L7.29791847,8.5 L4.92395924,10.8739592 C4.59201359,11.2059049 4.59201359,11.7440951 4.92395924,12.0760408 C5.25590488,12.4079864 5.79409512,12.4079864 6.12604076,12.0760408 L8.5,9.70208153 L10.8739592,12.0760408 C11.2059049,12.4079864 11.7440951,12.4079864 12.0760408,12.0760408 C12.4079864,11.7440951 12.4079864,11.2059049 12.0760408,10.8739592 L9.70208153,8.5 L12.0760408,6.12604076 C12.4079864,5.79409512 12.4079864,5.25590488 12.0760408,4.92395924 C11.7440951,4.59201359 11.2059049,4.59201359 10.8739592,4.92395924 L8.5,7.29791847 L8.5,7.29791847 Z", fill: "#FFF"}
